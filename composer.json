{
  "name": "dragonmantank/zendexpressive-session-middleware",
  "description": "Provides a session middleware for Zend Expressive",
  "keywords": [
    "zendexpressive-session-middleware",
    "psr-7",
    "zend",
    "session",
    "middleware"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Tankersley",
      "email": "chris@ctankersley.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-session": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Dragonmantank\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dragonmantank\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
